<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.BookLendingDao">
    <insert id="insertBookLendingRecord" parameterType="com.example.demo.Entity.BookLending">
        insert into `book_lending`(`user_id`, `book_id`, `ldate`, `ddate`) values(#{userId}, #{bookId}, #{lendDate}, #{dueDate})
    </insert>
</mapper>